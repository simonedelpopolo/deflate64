{
  "name": "deflate64",
  "description": "Command line utility to convert one file/string into compressed base64 encoding. the file/string can be decompressed and decoded to its original status.",
  "version": "2.1.11-beta",
  "type": "module",
  "workspaces": [
    "website"
  ],
  "engines": {
    "node": ">=v16.13.1 <17",
    "npm": "~8.3.0"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "main": "./index.js",
  "bin": {
    "d64": "./executable.js",
    "deflate64": "./executable.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "assertions": "cd ./assertions/ && node ./assertions.js",
    "assertion0": "cd ./assertions/ && node ./assertions.js assertion0",
    "assertion1": "cd ./assertions/ && node ./assertions.js assertion1",
    "assertionOnGoing": "cd ./assertions/ && node ./assertions.js assertionOnGoing"
  },
  "dependencies": {
    "boolean-jokes": "^1.3.4",
    "json-swiss-knife": "^1.5.7",
    "oftypes": "^0.3.0-beta"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-plugin-jsdoc": "^37.5.2"
  },
  "publishConfig": {
