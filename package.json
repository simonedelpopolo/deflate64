{
  "name": "deflate64",
  "description": "Command line utility to convert one file/string into compressed base64 encoding. the file/string can be decompressed and decoded to its original status.",
  "version": "1.3.12",
  "type": "module",
  "engines": {
    "node": ">=v16.13.1 <17",
    "npm": "~8.3.0"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "main": "./index.js",
  "bin": {
    "deflate64": "./executable.js"
  },
  "scripts": {
    "jest": "NODE_OPTIONS='--experimental-vm-modules' npx jest",
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "assertions": "cd ./assertions/ && node ./assertions.js",
    "assertion0": "cd ./assertions/ && node ./assertions.js assertion0",
    "assertion1": "cd ./assertions/ && node ./assertions.js assertion1"
  },
  "dependencies": {
    "@simonedelpopolo/json-parse": "^1.3.1",
    "@simonedelpopolo/to-bool": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-plugin-jsdoc": "^37.2.2",
    "jest": "^27.4.4",
    "jest-extended": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/simonedelpopolo/deflate64#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/deflate64.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/deflate64/issues"
  },
  "keywords": [
    "base64",
    "ziplib",
    "file",
    "deflate",
    "inflate",
    "string",
    "automation"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "license": "Apache-2.0"
}
