{
  "name": "deflate64",
  "version": "0.3.2",
  "type": "module",
  "engines": {
    "node": ">=v16.13.1 <17",
    "npm": "~8.3.0"
  },
  "description": "Command line utility to convert one file/string into compressed base64 encoding. the file/string can be decompressed and decoded to its original status.",
  "main": "./index.js",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' pnpx jest",
    "lint": "pnpx eslint . --ext .js",
    "assertions": "cd ./assertions/ && node ./assertions.js"
  },
  "bin": {
    "deflate64": "./deflate64.js"
  },
  "keywords": [
    "base64",
    "ziplib",
    "file",
    "deflate",
    "inflate",
    "string"
  ],
  "author": "Simone Del Popolo",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^8.4.1",
    "jest": "^27.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/deflate64.git"
  },
  "bugs": {
    "url": "https://github.com/simonedelpopolo/deflate64/issues"
  },
  "homepage": "https://github.com/simonedelpopolo/deflate64#readme",
  "dependencies": {
    "@simonedelpopolo/json-parse": "^1.2.0",
    "@simonedelpopolo/to-bool": "^1.0.1"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
